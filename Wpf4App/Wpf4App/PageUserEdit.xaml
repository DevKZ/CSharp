<Page x:Class="Wpf4App.PageUserEdit"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Wpf4App"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="PageUserEdit">

    <Grid>
        <Button Content="追加" HorizontalAlignment="Left" Height="48" Margin="50,50,0,0" VerticalAlignment="Top" Width="80" Click="btnAdd_Click"/>
        <Button Content="削除" HorizontalAlignment="Left" Height="48" Margin="50,100,0,0" VerticalAlignment="Top" Width="80" Click="btnDel_Click"/>
        <Button Content="登録" HorizontalAlignment="Left" Height="48" Margin="50,150,0,0" VerticalAlignment="Top" Width="80" Click="btnRegist_Click"/>
        <Button Content="戻る" HorizontalAlignment="Left" Height="48" Margin="50,250,0,0" VerticalAlignment="Top" Width="80" Click="btnBack_Click"/>

    </Grid>
</Page>
